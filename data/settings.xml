<!DOCTYPE settings SYSTEM 'settings.dtd'>
<settings>

  <settings>

    <setting id="accuracy">screen</setting>
    <setting id="blur">0</setting>
    <setting id="consolebackground">skins/dipohlo/console.png</setting>
    <setting id="default_machine">Gradiente_Expert_DD_Plus</setting>
    <setting id="di_halt_callback">dihaltcallback</setting>
    <setting id="fullspeedwhenloading">true</setting>
    <setting id="horizontal_stretch">320.0</setting>
    <setting id="invalid_psg_directions_callback">psgdirectioncallback</setting>
    <setting id="osd_leds_set">dipohlo</setting>
    <setting id="pause_on_lost_focus">false</setting>
    <setting id="save_settings_on_exit">false</setting>
    <setting id="scale_factor">3</setting>
    <setting id="scanline">100</setting>

  </settings>

  <bindings>
    <bind key="keyb F4">record start -doublesize -stereo</bind>
    <bind key="keyb F5">record stop</bind>
    <bind key="keyb F6">toggle_osd_keyboard</bind>
  </bindings>

</settings>
