#!/bin/bash

# Arkanon <arkanon@lsd.org.br>
# 2015/08/15 (SÃ¡b) 21:00:16 BRT

  cs()
  {
     c=$2
     l=$3
    lt=$4
    at=$5
    lc=$(bc<<<"scale=2;($lt-4)/$c")
    ac=$(bc<<<"scale=2;($at  )/$l")
    LC_ALL=C printf "%5.2f  %5.2f\n" $lc $ac
  }

#     tf    c   l   lt   at  e   t  d     lc  ac
#                  960  720
  cs   8  191  72  959  720  0   0  1  #   5  10
  cs   9  191  65  959  715  0   5  1  #   5  11

  cs  10  159  60  958  720  1   0  1  #   6  12

  cs  11  136  55  956  715  2   5  2  #   7  13
  cs  12  136  51  956  714  2   6  2  #   7  14
  cs  13  119  45  956  720  2   0  2  #   8  16
  cs  14  119  42  956  714  2   6  2  #   8  17

  cs  15  106  40  958  720  1   0  1  #   9  18

  cs  16   95  37  954  703  3  17  3  #  10  19
  cs  17   95  36  954  720  3   0  3  #  10  20
  cs  18   86  34  950  714  5   6  5  #  11  21
  cs  19   86  31  950  713  5   7  5  #  11  23

  cs  20   79  30  952  720  4   0  4  #  12  24

  cs  21   73  28  953  700  3  20  4  #  13  25
  cs  22   73  27  953  702  3  18  4  #  13  26
  cs  23   68  26  956  702  2  18  2  #  14  27
  cs  24   68  25  956  700  2  20  2  #  14  28

  cs  32   50  18  954  684  3  36  3  #  19  38

